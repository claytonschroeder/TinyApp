<!-- views/urls_login.ejs -->
<% include partials/_head %>

    <main>
        <div class="jumbotron" style="background-color:green;">
            <h3 style="color:white;">Please enter your Username and Password</h3>
            <form action="/login" method="POST">
              <label for="email"></label>
                <br><input id="email" type="email" name="email" placeholder="enter email" style="width: 300px"></input><br>
                <br><input id="password" type="password" name="password" placeholder="enter password" style="width: 300px"></input><br><br>
              <input type="submit" value="Submit"></input><br>
            </form>
        </div>
    </main>

<% include partials/_footer %>
